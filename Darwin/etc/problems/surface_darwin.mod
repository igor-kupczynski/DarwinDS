var[-2.0, 2.0] x;
var[-2.0, 2.0] y;
var[-2.0, 2.0] z;

min f1: x;
min f2: y;
min f3: z;

c1: -x + y + 0.6 >= 0;
c2:  x + z - 0.5 >= 0;
c3:  x + y + z - 1.1 >= 0;
l1a: x >= 0;
l2a: y >= 0;
l3a: z >= 0;
l1b: x <= 1;
l2b: y <= 1;
l3b: z <= 1;

!dec: (-1.000000 * f1) + (-2.000000 * f2)  + (-1.000000 * f3);